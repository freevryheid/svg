program tst

real a(10,2)
integer i
character(len=16) :: nbr
character(len=:), allocatable :: fin
do i = 1,10
  a(i,1) = real(i)
  a(i,2) = real(i*i)
end do


fin = ""
do i=1,10
  write(nbr, '(f16.4)') a(i,1)
  fin = fin // trim(adjustl(nbr)) // ","
  write(nbr, '(f16.4)') a(i,2)
  fin = fin // trim(adjustl(nbr)) // " "
end do

print *, fin

end program tst
